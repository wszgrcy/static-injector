{
  "name": "static-injector",
  "version": "0.0.0",
  "description": "Angular 依赖注入独立版本;Angular dependency injection standalone version",
  "private": true,
  "scripts": {
    "sync": "code-recycle ./script/sync/index.ts --cwd ./src && eslint --fix ./src &&  prettier ./src --write",
    "test": "rimraf -g ./test-dist/**/*.mjs && tsx ./script/test.esbuild.ts && mocha \"./test-dist/**/*.spec.mjs\" --enable-source-maps --inspect",
    "coverage": "c8 npm run test",
    "build": "rimraf dist && npm run build:import",
    "build:import": "tsx ./script/build && tsc -p tsconfig.type.json && cpx ./assets/package.json ./dist && cpx -v ./readme.md ./dist"
  },
  "author": "wszgrcy",
  "license": "MIT",
  "devDependencies": {
    "@code-recycle/cli": "1.3.10",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.30",
    "c8": "^10.1.3",
    "chai": "^5.2.0",
    "cpx": "^1.5.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.21.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fast-glob": "^3.3.3",
    "globals": "^16.0.0",
    "husky": "^7.0.1",
    "mocha": "^11.1.0",
    "prettier": "^3.5.2",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.25.0"
  }
}
